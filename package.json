{
  "name": "pollin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear": "rm -rf .next && rm -rf dist",
    "build": "npm run clear && next build && npm run copy:assets",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "copy:assets": "mkdir dist && cp -R .next dist/.next && cp -R public dist/public && cp package.json dist && cp yarn.lock dist && cp app.yaml dist",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "test": "cypress run",
    "test:open": "cypress open",
    "cypress:run": "start-test dev 3000 test",
    "cypress:open": "start-test dev 3000 test:open",
    "view:cov": "open ./coverage/lcov-report/index.html",
    "cypress:report-html": "yarn run cypress:open && yarn run view:cov",
    "coverage": "nyc yarn run cypress:run"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fullcalendar/common": "^5.11.2",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.11.2",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/react": "^5.11.2",
    "@fullcalendar/timegrid": "^5.11.2",
    "@fullcalendar/timeline": "^5.10.1",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.95",
    "@mui/material": "^5.10.1",
    "@mui/styles": "^5.9.3",
    "@mui/system": "^5.10.1",
    "@mui/utils": "^5.9.3",
    "@mui/x-data-grid": "^5.15.3",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@tabler/icons": "^1.84.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "csstype": "^3.1.0",
    "date-fns": "^2.29.2",
    "formik": "^2.2.9",
    "framer-motion": "^7.2.0",
    "material-ui-popup-state": "^4.0.2",
    "next": "^12.2.5",
    "next-transpile-modules": "^9.0.0",
    "nyc": "^15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "sass": "^1.54.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/node": "^18.7.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.6.0",
    "danger": "^11.1.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.1",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": "^13.0.3",
    "next-compose-plugins": "^2.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.7.4"
  }
}
