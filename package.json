    "@mui/material": "^5.10.16",
    "@mui/styles": "^5.10.16",
    "@mui/system": "^5.10.16",
    "@mui/utils": "^5.10.16",
    "@mui/x-data-grid": "^5.17.14",
    "@mui/x-date-pickers": "^5.0.9",
    "@reduxjs/toolkit": "1.8.6",
    "@sentry/nextjs": "^7.23.0",
    "@tabler/icons": "^1.115.0",
    "axios": "^1.2.0",
